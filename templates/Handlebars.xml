<templateSet group="Handlebars">
  <template name="log" value="{{log $END$}}" description="{{log}} expansion" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="Handlebars" value="true" />
    </context>
  </template>
  <template name="itar" value="{{#each $HELPER$}}&#10;$END$&#10;{{/each}}" description="Handlebars Each block" toReformat="true" toShortenFQNames="false" deactivated="true">
    <variable name="HELPER" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
</templateSet>